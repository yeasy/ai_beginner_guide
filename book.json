{
  "title": "零基础学AI",
  "author": "yeasy",
  "language": "zh-hans",
  "plugins": [
    "mermaid-gb3",
    "chapter-fold",
    "back-to-top-button",
    "code",
    "splitter",
    "page-toc-button",
    "search-pro",
    "prism",
    "-lunr",
    "-search",
    "-highlight",
    "flexible-alerts"
  ],
  "pluginsConfig": {
    "page-toc-button": {
      "maxTocDepth": 3,
      "minTocSize": 2
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-solarizedlight.css"
      ]
    },
    "flexible-alerts": {
      "style": "callout",
      "note": {
        "label": "说明",
        "icon": "fa fa-info-circle",
        "className": "info"
      },
      "tip": {
        "label": "提示",
        "icon": "fa fa-lightbulb-o",
        "className": "tip"
      },
      "warning": {
        "label": "注意",
        "icon": "fa fa-exclamation-triangle",
        "className": "warning"
      },
      "danger": {
        "label": "警告",
        "icon": "fa fa-ban",
        "className": "danger"
      }
    }
  }
}